{
  "name": "turnagain-guide",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "gen:gpx": "node --experimental-strip-types lib/generate-geojson.ts && cat public/turnagain-pass.geojson | node --experimental-strip-types lib/gpx.ts > public/turnagain-pass.gpx",
    "gen:geojson": "node --experimental-strip-types lib/generate-geojson.ts",
    "gen": "pnpm run gen:geojson && pnpm run gen:gpx",
    "migrate:mdx": "uv run scripts/migrate-metadata-to-mdx.py",
    "dev": "next dev",
    "build": "pnpm run gen && next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "npx http-server out/ -p 4000"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@serwist/next": "^9.0.11",
    "@serwist/precaching": "^9.0.11",
    "@serwist/sw": "^9.0.11",
    "@types/geojson": "^7946.0.15",
    "@types/mdx": "^2.0.13",
    "cesium": "^1.127.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "fast-xml-parser": "^4.5.1",
    "front-matter": "^4.0.2",
    "lucide-react": "^0.479.0",
    "next": "15.1.3",
    "next-mdx-remote-client": "^2.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "serwist": "^9.0.11",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "protobufjs",
      "sharp"
    ]
  }
}
